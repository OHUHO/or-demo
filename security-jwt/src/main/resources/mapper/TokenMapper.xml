<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.record.security.mapper.TokenMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.record.security.entity.Token">
        <id column="id" property="id" />
        <result column="token" property="token" />
        <result column="token_type" property="tokenType" />
        <result column="revoked" property="revoked" />
        <result column="expired" property="expired" />
        <result column="user_id" property="userId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, token, token_type, revoked, expired, user_id
    </sql>

</mapper>
